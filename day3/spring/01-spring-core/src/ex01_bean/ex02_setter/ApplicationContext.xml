<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.2.xsd">
        



   <!--
      Define a simple bean, but this time provide initial values for properties
      on the JavaBean
   -->
 
    
  	
	<bean id="bean1" class="ex01_bean.ex02_setter.Bean1">
      
      <!--
         Assign initial value to each property
      -->
      <property name="no">
         <value>101</value>
      </property>
      
      <property name="name">
         <value>Boss</value>
      </property>
      
      <property name="hobbies">
         <set>
            <value>chess</value>
            <value>cricket</value>
            <value>reading</value>
         </set>
      </property>
      
	</bean>
	
	
	
	<bean id="bean2" class="ex01_bean.ex02_setter.Bean2">
      
      <!--
         Assign initial value to each property
      -->
      <property name="no">
         <value>1101</value>
      </property>
      
      <property name="name">
         <value>JBoss</value>
      </property>
      
      <property name="hobbies">
         <set>
            <value>chess</value>
            <value>cricket</value>
            <value>reading</value>
         </set>
      </property>
      
	</bean>
	
	
	
</beans>
